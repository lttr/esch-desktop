<BorderPane fx:id="rootPane" prefHeight="680.0" prefWidth="960.0" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cz.lttr.esch.controller.RootController">
